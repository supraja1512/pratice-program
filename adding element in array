source code:
#include<iostream>
using namespace std;
int main()
{
    int n,index,item;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>index;
    //cin>>item;
    for(int i=index;i<n-1;i++)        
    {
        //int temp;
        //temp=a[index]; 
        a[i]=a[i+1]; 
        //a[index+2]=a[index+1];
    }
    
    for(int i=0;i<n-1;i++)    
    {
        cout<<a[i]<<"\t";
    }
}
